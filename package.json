{
  "name": "cross-domain-emitter",
  "version": "1.0.1",
  "description": "a cross domain or carrier event emittier",
  "main": "dist/index.min.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "pretest": "pnpm run build",
    "lint": "eslint --fix **/*.{ts,jsx,js,vue} & npm run format",
    "format": "npx prettier --write .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "pnpm run build && npx jest",
    "test:server": "cd ./tests/webserver && pnpm i && pnpm run dev",
    "prepublish": "pnpm run build",
    "prepare": "npx husky install"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wxwzl/cross-domain-emitter.git"
  },
  "types": "dist",
  "typings": "dist",
  "exports": {
    ".": {
      "import": "./dist/index.min.esm.js",
      "require": "./dist/index.min.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "cross",
    "domain",
    "event",
    "emitter",
    "cross",
    "carrier"
  ],
  "author": "wxwzl",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-eslint": "^8.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "^29.0.1",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.0.3",
    "jest-environment-jsdom": "^29.0.2",
    "lint-staged": "^13.0.3",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "29.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@wxwzl/eventemitter": "1.0.1",
    "nanoid": "4.0.0"
  }
}
